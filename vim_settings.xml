      <register name="0" type="4">
        <text>if self.use_dc:
            n_samples, n_features = X.shape

            # calculate term frequencies, f(t), +1 to smooth
            tfs = np.asarray(X.sum(axis=0)).ravel() + 1

            # initialize dc
            dc = np.ones(n_features)

            # all labels
            labels = set(y)
            for label in labels:
                label_X = X[y == label]  # slicing data of specific label
                label_tfs = np.asarray(label_X.sum(axis=0)).ravel() + 1  # f(t, Ci), +1 to smooth
                pt_label = label_tfs / tfs  # f(t, Ci) / f(t)
                dc += pt_label * np.log(pt_label) / np.log(len(labels))

            self._dc_diag = sp.spdiags(dc, diags=0, m=n_features, n=n_features, format='csr')</text>
